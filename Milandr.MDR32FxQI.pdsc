            <accept condition="CON_MDR32F1QI"/>
        </condition>
        <condition id="CON_MDR32F1QI_ETH_DEPENDENCIES_DMA_RST_CLK_UTILS">
            <description>MDR32F1QI device with ETH dependencies (DMA, RST_CLK and UTILS)</description>
